{
  "expo": {
    "name": "mata-fome-mobile",
    "slug": "mata-fome-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/public/icons/tomato/TomatoAssassin.png",
    "scheme": "mata-fome-mobile",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/public/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff7f00"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.giovaniflp.matafomemobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "react-native-map-link",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e2ab8085-b6d4-42d4-9dab-5a8626776a0b"
      }
    }
  }
}
